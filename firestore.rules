rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // 1. Allow anyone to register (create) and anyone to view (read)
      // 2. This rule allows editing and deleting from the dashboard.
      // 3. This rule NEVER expires.
      allow read, create, update, delete: if true;
    }
  }
}
